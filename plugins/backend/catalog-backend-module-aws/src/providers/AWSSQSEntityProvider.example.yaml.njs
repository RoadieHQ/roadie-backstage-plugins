kind: Resource
apiVersion: backstage.io/v1beta1
metadata:
  name: {{ data.QueueArn | split(":") | last }}
  title: {{ data.QueueArn | split(":") | last }}
  queueArn: {{ data.QueueArn }}
  annotations:
    amazon.com/sqs-queue-arn: {{ data.QueueArn }}
  labels:
    aws-sqs-region: {{ region }}
spec:
  type: 'sqs-queue'