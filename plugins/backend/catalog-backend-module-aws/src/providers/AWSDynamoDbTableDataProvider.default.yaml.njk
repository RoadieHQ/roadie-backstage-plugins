kind: {{ additionalData.entityKind or 'Component' }}
apiVersion: backstage.io/v1beta1
metadata:
  name: "{{ data[additionalData.identifierColumn] | lower | sanitize_name }}"
  title: "{{ data[additionalData.identifierColumn] }}"
  annotations:
    {% for key, value in mergedAnnotations %}
    "{{ key }}": "{{ value }}"
    {% endfor %}
spec:
  owner: "{{ accountId }}"
  type: dynamo-db-table-data
  lifecycle: production